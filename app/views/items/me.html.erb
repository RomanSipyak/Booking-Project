<% @items.each do |item| %>
  <div class="container">
    <h2><%= item.category.title %></h2>
    <div class = "row">
      <div class="col-md-1">
        <div class="row">
          <div class="col-md-6">
            <%= link_to "Delete(not work)", destroy_user_session_path, method: :delete, class: "btn btn-primary"%>
          </div>
        </div>
        <div class="row">
          <div class="col-md-6">
            <%= link_to "Update(not work)", destroy_user_session_path, method: :delete, class: "btn btn-primary"%>
          </div>
        </div>
      </div>
      <div class="col-md-6">
        <ul class="list-group">
          <li class="list-group-item">Price:<%= item.price %></li>
          <li class="list-group-item">Description:<%= item.description %></li>
        </ul>
      </div>
      <div class="col-md-3">
        <%= link_to "Show all books(not work)", destroy_user_session_path, method: :delete, class: "btn btn-success"%>
      </div>
    </div>
  </div>
<% end %>